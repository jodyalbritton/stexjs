{
    "name": "sample-es6-library",
    "version": "0.0.2",
    "description": "Sample ES6 library",
    "author": "frankandrobot",
    "homepage": "https://github.com/frankandrobot/sample-es6-library",
    "repository": "frankandrobot/sample-es6-library",
    "main": "dist/legacy-umd/index.js",
    "module": "src/index.js",
    "scripts": {
        "clean": "rimraf dist && mkdirp dist/legacy && mkdirp dist/legacy-umd",
        "dev": "SET BABEL_ENV=browser&&SET NODE_ENV=development&&babel-node ./webpack/dev-server.js",
        "build:legacy": "SET BABEL_ENV=legacy babel src --source-maps --no-comments --out-dir dist/legacy/",
        "build:legacy-umd": "rollup --config",
        "build": "npm run clean && run-p build:**"
    },
    "license": "MIT",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "mkdirp": "^0.5.1",
        "npm-run-all": "^4.1.1",
        "rimraf": "^2.6.2",
        "rollup": "^0.49.1",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-commonjs": "^8.2.0",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-sourcemaps": "^0.4.2"
    },
    "dependencies": {
        "babel-runtime": "^6.26.0",
        "columnify": "^1.5.4",
        "request": "^2.83.0",
        "request-promise": "^4.2.2",
        "request-promise-native": "^1.0.5",
    }
}
