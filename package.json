{
  "name": "stexjs",
  "version": "0.1.1",
  "description": "STexJs",
  "author": "Jody Albritton",
  "homepage": "https://github.com/jodyalbritton/stexjs",
  "repository": "jodyalbritton/stexjs",
  "main": "dist/legacy/index.js",
  "module": "src/index.js",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist/legacy && mkdirp dist/legacy-umd",
    "build:legacy": "BABEL_ENV=legacy babel src --source-maps --no-comments --out-dir dist/legacy/",
    "build:legacy-umd": "rollup --config",
    "build": "npm run clean && run-p build:**"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5"
  }
}
