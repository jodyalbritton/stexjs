{
  "name": "stexjs",
  "version": "0.1.1",
  "description": "STexJs",
  "author": "Jody Albritton",
  "homepage": "https://github.com/jodyalbritton/stexjs",
  "repository": "jodyalbritton/stexjs",
  "main": "dist/legacy/index.js",
  "module": "src/index.js",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist/legacy && mkdirp dist/legacy-umd",
